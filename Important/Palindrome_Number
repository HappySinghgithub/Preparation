                  //   	Palindrome Number By Naresh I Technologies
#include<stdio.h>
void main()
{
    int n,r,sum=0,temp;
    printf("Enter the number: ");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        sum = sum*10+r;
        n=n/10;
    }
    n=temp;
    if(n==sum)
    printf("Palindrome number");
    else
    printf("Not Palindrome number");
}
